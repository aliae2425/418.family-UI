<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:FamiliBrowser_UI.MVVM.ViewModels"
                    xmlns:view="clr-namespace:FamiliBrowser_UI.MVVM.Views">

    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <view:HomeView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AccountViewModel}">
        <view:AccountView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AdminViewModel}">
        <view:AdminView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:FamilyViewModel}">
        <view:FamilyView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:FavorisViewModel}">
        <view:FavorisView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LoginViewModel}">
        <view:LoginView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingsViewModel}">
        <view:SettingsView />
    </DataTemplate>
    
</ResourceDictionary>